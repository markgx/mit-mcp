{
  "name": "mit-mcp",
  "version": "0.0.1",
  "main": "dist/src/index.js",
  "type": "module",
  "typings": "dist/src/index.d.ts",
  "bin": {
    "mit-mcp": "dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markgx/mit-mcp.git"
  },
  "keywords": [
    "mcp"
  ],
  "author": "Mark Guerra <mark@markgx.dev>",
  "license": "MIT",
  "scripts": {
    "bundle": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --platform=node --target=node20.10.0 --format=esm ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --watch test --ext ts,json --exec 'npm run bundle && npm run start'",
    "debug": "npm run bundle && node --enable-source-maps --inspect-brk ./dist/index.js",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "node --enable-source-maps ./dist/index.js",
    "test": "jest --runInBand ."
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@jest/globals": "30.0.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/jest": "29.5.14",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "esbuild": "^0.25.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.14.0",
    "jest": "30.0.1",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "rimraf": "5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.1",
    "zod": "^3.25.67"
  }
}
